{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "es2020",
    "esModuleInterop": true,
    "checkJs": true,
    "paths": {
      "@/*": [
        "./*"

      ],
      "@components/*": ["components/*"],
      "@/hooks/*": ["hooks/*"],
      "@/utils/*": ["app/utils/*"],
      "@/assets/*": ["assets/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
